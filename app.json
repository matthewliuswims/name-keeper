{
  "expo": {
    "name": "Name Keeper",
    "slug": "name-keeper",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "version": "3.2.12",
    "orientation": "portrait",
    "icon": "./assets/name-keeper-app-icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.matthewliuswims.namekeeper"
    },
    "android": {
      "package": "com.matthewliuswims.namekeeper"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "matthew-liu",
            "project": "remember-names",
            "authToken": "246d247fbcad4da38dda93e3715b75f7149e2182a104425aa6683a5a5320a0a7"
          }
        }
      ]
    },
    "description": "App that stores the names + descriptions of the people you meet within your social groups.",
    "githubUrl": "https://github.com/matthewliuswims/name-keeper"
  }
}